<div class="item github" data-timestamp="{{timestamp}}">
  <div class="info">
    <div class="date">
      <h2>
        {{date}}
      </h2>
    </div>
    <div class="icon icon-github purple"></div>
  </div>
  <div class="content">
    <h2 class="title">
      <a href="//github.com/dansowter">dansowter</a>
      <span>pushed</span> to 
      <a href="//github.com/"+{{repo}} class="css-truncate css-truncate-target">{{repo}}</a>
    </h2>
    <ul class="commits">
      {{#each commits}}
        <li>
          <span class="sha-icon icon-flow-branch" ></span>
          <a href={{url}} class="sha">{{this.sha}}</a>
          <span class="message">{{this.message}}</span>
        </li>
      {{/each}}
    </ul>
  </div>
</div>
